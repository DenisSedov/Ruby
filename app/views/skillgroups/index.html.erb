<h1>Listing skillgroups</h1>

<table>
  <tr>
    <th>Seniorid</th>
    <th>Skill</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @skillgroups.each do |skillgroup| %>
  <tr>
    <td><%= skillgroup.seniorid %></td>
    <td><%= skillgroup.skill %></td>
    <td><%= link_to 'Show', skillgroup %></td>
    <td><%= link_to 'Edit', edit_skillgroup_path(skillgroup) %></td>
    <td><%= link_to 'Destroy', skillgroup, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Skillgroup', new_skillgroup_path %>
