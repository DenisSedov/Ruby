<h1>Listing workers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Fname</th>
    <th>Oname</th>
    <th>Contactinfo</th>
    <th>Status</th>
    <th>Desiralesalary</th>
    <th>Skill</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @workers.each do |worker| %>
  <tr>
    <td><%= worker.name %></td>
    <td><%= worker.fname %></td>
    <td><%= worker.oname %></td>
    <td><%= worker.contactinfo %></td>
    <td><%= worker.status %></td>
    <td><%= worker.desiralesalary %></td>
    <td><%= worker.skill %></td>
    <td><%= link_to 'Show', worker %></td>
    <td><%= link_to 'Edit', edit_worker_path(worker) %></td>
    <td><%= link_to 'Destroy', worker, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Worker', new_worker_path %>
